{
    "name": "pymedphys",
    "author": {
      "name": "PyMedPhys",
      "email": "developers@pymedphys.com",
      "url": "https://pymedphys.com"
    },
    "licence": "Apache-2.0",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://pymedphys.com",
    "main": "app/main.js",
    "build": {
      "productName": "PyMedPhys",
      "appId": "com.pymedphys.app",
      "files": [
        "app/**/*",
        "node_modules/**/*",
        "package.json"
      ],
      "directories": {
        "buildResources": "resources"
      },
      "extraResources": [
        {
          "from": "python",
          "to": "python",
          "filter": [
            "**/*"
          ]
        }
      ],
      "publish": null
    },
    "scripts": {
      "postinstall": "electron-builder install-app-deps",
      "preinstall": "set PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD=1",
      "start": "node scripts/start.js",
      "build": "webpack --config=scripts/webpack.app.config.js --env=production && electron-builder",
      "test": "playwright test"
    },
    "dependencies": {
      "@grpc/grpc-js": "^1.3.7",
      "fs-jetpack": "^4.1.0",
      "google-protobuf": "^3.18.0",
      "promise-delegate": "^1.0.1"
    },
    "devDependencies": {
      "@babel/core": "^7.5.5",
      "@babel/preset-env": "^7.5.5",
      "@playwright/test": "^1.16.3",
      "electron": "15.0.0",
      "electron-builder": "^22.11.11",
      "electron-rebuild": "^3.2.3",
      "grpc-tools": "^1.11.2",
      "playwright": "^1.16.3",
      "source-map-support": "^0.5.6",
      "ts-loader": "^9.2.6",
      "typescript": "^4.4.3",
      "wait-on": "^6.0.0",
      "webpack": "^5.30.0",
      "webpack-cli": "^4.6.0",
      "webpack-merge": "^5.7.3",
      "webpack-node-externals": "^3.0.0"
    }
  }
